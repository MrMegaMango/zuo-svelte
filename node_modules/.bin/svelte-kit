#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mango/code/zuo-svelte/node_modules/.pnpm/@sveltejs+kit@2.17.3_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.20.4_vite@6.2.0_@types_68636bfe2091cd7b40847209042ec95d/node_modules/@sveltejs/kit/node_modules:/home/mango/code/zuo-svelte/node_modules/.pnpm/@sveltejs+kit@2.17.3_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.20.4_vite@6.2.0_@types_68636bfe2091cd7b40847209042ec95d/node_modules/@sveltejs/node_modules:/home/mango/code/zuo-svelte/node_modules/.pnpm/@sveltejs+kit@2.17.3_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.20.4_vite@6.2.0_@types_68636bfe2091cd7b40847209042ec95d/node_modules:/home/mango/code/zuo-svelte/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mango/code/zuo-svelte/node_modules/.pnpm/@sveltejs+kit@2.17.3_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.20.4_vite@6.2.0_@types_68636bfe2091cd7b40847209042ec95d/node_modules/@sveltejs/kit/node_modules:/home/mango/code/zuo-svelte/node_modules/.pnpm/@sveltejs+kit@2.17.3_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.20.4_vite@6.2.0_@types_68636bfe2091cd7b40847209042ec95d/node_modules/@sveltejs/node_modules:/home/mango/code/zuo-svelte/node_modules/.pnpm/@sveltejs+kit@2.17.3_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.20.4_vite@6.2.0_@types_68636bfe2091cd7b40847209042ec95d/node_modules:/home/mango/code/zuo-svelte/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
